@page
@model ZewailCiryScienceWeek.Pages.registrarvisitorModel
@{
    Layout = "_Layoutregister";
}
<section class="section" id="about" ; style="background-color:#E5EBEB">
<div class="col-lg-6 col-md-12 col-sm-12">
    <div class="centered-text">
        <h2 Style="font-weight:900">View all data about visitors</h2>
    </div>
    <br>
    <br><br>
        <table id="popup-content" style="margin-bottom:200px">
        <thead>
            <tr>
                <th>National ID</th>
                <th>Age</th>
                <th>Sex</th>
                <th>How You Know Us</th>
                <th>Ticket ID</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.dt.Rows.Count; i++)
            {
                <tr>
                    <th scope="row">@(i + 1)</th>
                    @for (int j = 0; j < @Model.dt.Columns.Count; j++)
                    {
                        <td>@Model.dt.Rows[i][j]</td>
                    }
                    
                </tr>
            }
        </tbody>
            
        </table>
</div>
</section>
